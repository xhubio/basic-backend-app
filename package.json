{
  "name": "basic-backend-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run src_format --fix && npm run src_lint --fix && babel src -d lib --fix",
    "src_lint": "eslint src tests",
    "src_format": "prettier --no-semi --single-quote --trailing-comma es5 --write \"{src,tests}/**/*.js\"",
    "test_only": "jest",
    "test": "npm run build && jest -c jest.config.json",
    "debug": "npm run build && node --inspect-brk ./node_modules/.bin/jest -i --config=./jest.config.json",
    "prepare": "npm run build",
    "start": "node app.js"
  },
  "dependencies": {
    "@xhubio/table-common": "^2.0.0",
    "@xhubio/table-import-spreadsheet-all": "^1.1.0",
    "@xhubio/table-import-spreadsheet-decision": "^1.0.4",
    "@xhubio/table-import-spreadsheet-matrix": "^1.0.5",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "fs": "0.0.1-security",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "multiparty": "^4.1.3",
    "promise": "^8.0.1",
    "serve-favicon": "~2.4.5",
    "stringify": "^5.2.0",
    "xlsx": "^0.12.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "clone": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-babel": "^4.1.2",
    "jest": "^22.0.1",
    "jest-cli": "^22.0.4",
    "jest-environment-node-debug": "^2.0.0",
    "jsonfile": "^4.0.0",
    "prettier": "^1.9.2",
    "regenerator-runtime": "^0.11.1",
    "semantic-release": "^12.4.1"
  }
}
